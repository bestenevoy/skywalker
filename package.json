{
    "name": "windrunner",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "packages/page",
        "packages/pipe",
        "packages/server"
    ],
    "scripts": {
        "page:dev": "cd packages/page && npm run dev",
        "page:build": "cd packages/page && cross-env NODE_ENV=production npm run build",
        "page:start": "cd packages/page && cross-env NODE_ENV=production npm start"
    },
    "dependencies": {
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/webpack": "^4.41.25",
        "cross-env": "^7.0.3"
    }
}
